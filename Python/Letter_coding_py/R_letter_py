#python 3.5.2 letter_R
for r in range(7):
    for c in range(5):
        if (c==0 and (r>=0 and r<7)) or ((r==0 or r==2) and(c>0 and c<3)) or(r==1 and c==4) or (r==3 and c==1) or (r==4 and c==2) or (r==5 and c==3) or (r==6 and c==4)  :
            print("* ",end="")
        else:
            print(end=" ")
    print()    
